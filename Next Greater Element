class Solution(object):
    def nextGreaterElement(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        d={n:i for i,n in enumerate(nums1)}
        res=[-1]*len(nums1)
        stack=[]
        for i in range(len(nums2)):
            curr=nums2[i]
            while stack and curr> stack[-1]:
                val=stack.pop()
                index=d[val]
                res[index]=curr
            if curr in nums1:
                stack.append(curr)
            
        return res
